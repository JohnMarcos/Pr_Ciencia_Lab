<h1>Mis Cursos</h1>

<h1><%= current_user.first_name %> <%= current_user.last_name %></h1>

<%= @course_enrollments.each do |e| %>
<h2>Enrrolado: <%= e.course_id %></h2>
<h2>Cursos: <%= @courses.select { |course| course.id == e.course_id }[:name] %></h2>
<% end %>
<%= link_to "Volver a Home", root_path %>
